{
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "files": [
            "src/Core/Translation/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "zendframework/zendframework1": "1.12.*",
        "doctrine/common": "2.5.*",
        "doctrine/orm": "2.5.*",
        "doctrine/annotations": "~1.2",
        "doctrine/collections": "1.2",
        "gedmo/doctrine-extensions": "~2.4",
        "symfony/event-dispatcher": "~2.3",
        "symfony/finder": "~2.3",
        "symfony/translation": "~2.3",
        "symfony/yaml": "~2.3",
        "myclabs/acl": "1.2.*",
        "myclabs/array-comparator": "1.1.*",
        "myclabs/xport": "0.2.*",
        "myclabs/php-enum": "1.*",
        "myclabs/deep-copy": "1.*",
        "myclabs/units-api": "dev-master",
        "myclabs/muih": "0.1.*",
        "myclabs/work": "~0.3.0",
        "mnapoli/doctrine-translated": "dev-master",
        "mnapoli/number-two": "~0.2.0",
        "php-di/php-di": "~5.0",
        "ocramius/proxy-manager": "~1.0",
        "php-di/zf1-bridge": "*",
        "psr/log": "~1.0",
        "monolog/monolog": "~1.17",
        "netcarver/textile": "~3.5",
        "dompdf/dompdf": "dev-master",
        "phpoffice/phpexcel": "dev-WriterExcel5OptionalConstructor as 1.7.99",
        "videlalvaro/php-amqplib": "~2.6"
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "behat/mink": "*",
        "behat/mink-extension": "*",
        "behat/mink-selenium2-driver": "*",
        "mnapoli/metaconsole": "dev-master",
        "phpunit/phpunit": "4.8.*"
    },
    "suggest": {
        "shvetsgroup/parallelrunner": "Run Behat scenarios in parallel"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git://github.com/myclabs/PHPExcel.git"
        },
        {
            "type": "git",
            "url": "git://github.com/myclabs/DoctrineExtensions.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/myclabs/Units-API.git"
        }
    ]
}

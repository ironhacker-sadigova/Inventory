            <tuv xml:lang="fr">
                <seg>La bibliothèque a été créée.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>The library has been created.</seg>
            </tuv>
        </tu>

        <tu tuid="libraryDeletionError">
            <tuv xml:lang="fr">
                <seg>La bibliothèque ne peut pas être supprimée car elle n'est pas vide.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>The library can't be deleted because it isn't empty.</seg>
            </tuv>

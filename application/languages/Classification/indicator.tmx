            <tuv xml:lang="en">
                <seg>Indicator</seg>
            </tuv>
        </tu>
        <tu tuid="indicators">
            <tuv xml:lang="fr">
                <seg>Indicateurs</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Indicators</seg>
            </tuv>
        </tu>

        <!-- Popup d'ajout -->


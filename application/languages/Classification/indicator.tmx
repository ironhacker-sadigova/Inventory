            <tuv xml:lang="en">
                <seg>This indicator cannot be deleted, since (at least) one context indicator relates to it.</seg>
            </tuv>
        </tu>

        <tu tuid="libraryCreationTitle">
            <tuv xml:lang="fr">
                <seg>Création d'une nouvelle bibliothèque de classification</seg>
            </tuv>

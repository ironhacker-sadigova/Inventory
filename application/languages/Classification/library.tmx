<?xml version="1.0" encoding="UTF-8"?>
<tmx version="2.0" xmlns="http://www.lisa.org/tmx20">
    <header adminlang="en" creationtool="locale4j"
            creationtoolversion="1.1" o-tmf="unknown" segtype="block" srclang="*all*"/>
    <body>

        <tu tuid="libraryCreationTitle">
            <tuv xml:lang="fr">
                <seg>Création d'une nouvelle bibliothèque de classification</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Creation of a new classification library</seg>
            </tuv>
        </tu>
        <tu tuid="libraryCreationHelp">
            <tuv xml:lang="fr">
                <seg>La nouvelle bibliothèque sera créée dans le compte « [ACCOUNT] ».</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>La new library will be created in the "[ACCOUNT]" account.</seg>
            </tuv>
        </tu>

        <tu tuid="libraryCreated">
            <tuv xml:lang="fr">
                <seg>La bibliothèque a été créée.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>The library has been created.</seg>
            </tuv>
        </tu>

        <tu tuid="libraryDeletionError">
            <tuv xml:lang="fr">
                <seg>La bibliothèque ne peut pas être supprimée car elle n'est pas vide.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>The library can't be deleted because it isn't empty.</seg>
            </tuv>
        </tu>

    </body>
</tmx>

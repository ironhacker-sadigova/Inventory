<?xml version="1.0" encoding="UTF-8"?>
<tmx version="2.0" xmlns="http://www.lisa.org/tmx20">
	<header adminlang="en" creationtool="locale4j"
		creationtoolversion="1.1" o-tmf="unknown" segtype="block" srclang="*all*" />
	<body>

		<!-- Page d'une analyse -->

		<!-- Libellé "par défaut" pour une nouvelle analyse -->

		<tu tuid="newReportDefaultLabelPage">
			<tuv xml:lang="fr">
				<seg>Nouvelle analyse</seg>
			</tuv>
			<tuv xml:lang="en">
				<seg>New report</seg>
			</tuv>
		</tu>

		<!-- Messages de confirmation -->

		<tu tuid="reportConfigurationParsed">
			<tuv xml:lang="fr">
				<seg>Analyse effectuée.</seg>
			</tuv>
			<tuv xml:lang="en">
				<seg>Report available.</seg>
			</tuv>
		</tu>

        <!-- Messages d'erreur -->

        <tu tuid="invalidConfig">
            <tuv xml:lang="fr">
                <seg>L'analyse n'a pas pu être lancée, car la configuration est incomplète ou invalide.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>The report could not be processed, because of an invalid or incomplete configuration.</seg>
            </tuv>
        </tu>
        <tu tuid="unableToSaveChartImage">
            <tuv xml:lang="fr">
                <seg>Suite à une erreur de l'application le graphique ne peut pas être affiché.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Following a bug the chart cannot be displayed.</seg>
            </tuv>
        </tu>
        <tu tuid="reportLabelInvalid">
            <tuv xml:lang="fr">
                <seg>La configuration n'a pas pu être enregistrée, car le libellé saisi est vide.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Save failed, because the label cannot be empty.</seg>
            </tuv>
        </tu>

	</body>
</tmx>
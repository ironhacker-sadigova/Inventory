<?xml version="1.0" encoding="UTF-8"?>
<tmx version="2.0" xmlns="http://www.lisa.org/tmx20">
    <header adminlang="en" creationtool="locale4j"
        creationtoolversion="1.1" o-tmf="unknown" segtype="block" srclang="*all*" />
    <body>

        <!-- Titre de la page -->

        <tu tuid="pageTitleSuffix">
            <tuv xml:lang="fr">
                <seg> — modification du mot de passe</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg> — password change</seg>
            </tuv>
        </tu>

        <!-- Formulaire -->

        <tu tuid="oldPassword">
            <tuv xml:lang="fr">
                <seg>Ancien mot de passe</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Old password</seg>
            </tuv>
        </tu>   
        <tu tuid="newPassword">
            <tuv xml:lang="fr">
                <seg>Nouveau mot de passe</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>New password</seg>
            </tuv>
        </tu>   
        <tu tuid="confirmNewPassword">
            <tuv xml:lang="fr">
                <seg>Nouveau mot de passe (confirmation)</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>Please confirm new password</seg>
            </tuv>
        </tu>   

        <!-- Messages -->

        <tu tuid="passwordsAreNotIdentical">
            <tuv xml:lang="fr">
                <seg>Cette saisie du nouveau mot de passe n'est pas identique à la précédente.</seg>
            </tuv>
            <tuv xml:lang="en">
                <seg>This typing of the new password is not identical to the one above.</seg>
            </tuv>
        </tu>   

    </body>
</tmx>